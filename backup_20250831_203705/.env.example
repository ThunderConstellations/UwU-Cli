# UwU-CLI Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# AI Provider Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
AI_MODEL=gpt-4o-mini
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=4000

# Telegram Bot Configuration (if using Telegram integration)
TELEGRAM_BOT_TOKEN=1234567890:your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Email Configuration (if using email integration)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
SMTP_TO=recipient@example.com

# Feishu Configuration (if using Feishu integration)
FEISHU_APP_ID=your_feishu_app_id_here
FEISHU_APP_SECRET=your_feishu_app_secret_here

# Security Configuration
ENCRYPTION_KEY=your_32_character_encryption_key_here
JWT_SECRET=your_jwt_secret_here

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=uwu_cli.log

# Performance Configuration
CACHE_TTL=3600
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=30

# Development Configuration
DEBUG=false
VERBOSE=false
DRY_RUN=false