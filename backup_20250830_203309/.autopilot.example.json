{
  "enabled": false,
  "description": "UwU-CLI Autopilot Configuration Template",
  "version": "1.0.0",
  
  "ai_provider": {
    "type": "openrouter",
    "api_key": "YOUR_OPENROUTER_API_KEY_HERE",
    "model": "gpt-4o-mini",
    "temperature": 0.7,
    "max_tokens": 4000
  },
  
  "integrations": {
    "telegram": {
      "enabled": false,
      "bot_token": "1234567890:YOUR_BOT_TOKEN_HERE",
      "chat_id": "YOUR_CHAT_ID_HERE",
      "auto_respond": false,
      "command_prefix": "/uwu"
    },
    
    "email": {
      "enabled": false,
      "smtp_host": "smtp.gmail.com",
      "smtp_port": 587,
      "smtp_user": "your_email@gmail.com",
      "smtp_password": "YOUR_APP_PASSWORD_HERE",
      "smtp_to": "recipient@example.com",
      "auto_forward": false
    },
    
    "feishu": {
      "enabled": false,
      "app_id": "YOUR_FEISHU_APP_ID_HERE",
      "app_secret": "YOUR_FEISHU_APP_SECRET_HERE",
      "webhook_url": "YOUR_WEBHOOK_URL_HERE"
    }
  },
  
  "autopilot_features": {
    "auto_respond": false,
    "context_memory": true,
    "learning_mode": false,
    "safety_checks": true,
    "rate_limiting": true
  },
  
  "security": {
    "encryption_enabled": true,
    "encryption_key": "YOUR_32_CHARACTER_ENCRYPTION_KEY_HERE",
    "jwt_secret": "YOUR_JWT_SECRET_HERE",
    "allowed_ips": [],
    "max_requests_per_minute": 60
  },
  
  "logging": {
    "level": "INFO",
    "file": "autopilot.log",
    "max_size": "10MB",
    "backup_count": 5
  },
  
  "performance": {
    "cache_ttl": 3600,
    "max_concurrent_requests": 5,
    "request_timeout": 30,
    "retry_attempts": 3
  }
}